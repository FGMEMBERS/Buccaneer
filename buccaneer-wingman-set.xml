<?xml version="1.0"?>
<!--
************************************************************************
Blackburn Buccaneer simulation config. This files ties together 
all the components used by FGFS to represent the aircraft. Components include 
the flight data model, instrument panel, and external 3D model. Lots still relies
on the Hawker Hunter/

Ver 0.0.1



************************************************************************
-->
<PropertyList>
	<sim include="help.xml">
		<description type="string" >Blackburn Buccaneer S2(Wingman)</description>
		<author type="string">Vivian Meazza, AJ MacLeod. Melchior Franz</author>
		<status type="string">alpha</status>
		<virtual-cockpit archive="y">true</virtual-cockpit>
		<flight-model>yasim</flight-model>
		<aero>buccaneer-yasim</aero>
		<fuel-fraction>1.0</fuel-fraction>
		<systems>
			<autopilot>
				<path>Aircraft/Buccaneer/Systems/buccaneer-autopilot.xml</path>
			</autopilot>
			<electrical>
				<path></path>
			</electrical>
		</systems>
		<instrumentation>
			<path>Aircraft/Buccaneer/Systems/instrumentation.xml</path>
		</instrumentation>
		<ai>
			<aircraft>
				<exhaust type="bool">false</exhaust>
			</aircraft>
			<submodels>
				<contrails type="bool">false</contrails>
			</submodels>
			<scenario n="100">wingman_demo</scenario>
			<!--<scenario n="101">wingman2_demo</scenario>-->
		</ai>
		<submodels>
			<serviceable type="bool">true</serviceable>
			<path>Aircraft/Hunter/Models/hunter-submodels.xml</path>
		</submodels>
		<sound>
			<path>Aircraft/Buccaneer/Sounds/buccaneer-sound.xml</path>
		</sound>
		<!-- hide the 2D panel -->
		<panel>
			<visibility archive="y">false</visibility>
		</panel>
		<!-- no HUD -->
		<hud>
			<path>Huds/Default/default.xml</path>
			<visibility>false</visibility>
		</hud>
		<rendering>
			<redout>
				<parameters>
					<blackout-onset-g type="double">4.5</blackout-onset-g>
					<blackout-complete-g type="double">6.0</blackout-complete-g>
				</parameters>
			</redout>
		</rendering>
		<menubar>
			<default>
				<!--<menu n="6">
          <item n="3">
            <enabled type="bool">false</enabled>
          </item>
        </menu>-->
				<menu n="1000">
					<label>Buccaneer</label>
					<enabled type="bool">true</enabled>
					<item>
						<label>Toggle pilot model</label>
						<binding>
							<command>property-toggle</command>
							<property>/sim/model/buccaneer/config/show-pilot</property>
						</binding>
					</item>
					<item>
						<label>Toggle G-induced head movement</label>
						<binding>
							<command>property-toggle</command>
							<property>/sim/headshake/enabled</property>
						</binding>
					</item>
				</menu>
			</default>
		</menubar>
		<model>
			<path>Aircraft/Buccaneer/Models/buccaneer-model.xml</path>
			<livery>
				<variant type="string" >Side No 322</variant>
			</livery>
			<formation>
				<variant type="string">Diamond</variant>
			</formation>
			<buccaneer>
				<controls>
					<recuperator_check_safety type="float">0</recuperator_check_safety>
				</controls>
				<config>
					<show-pilot type="bool">true</show-pilot>
				</config>
			</buccaneer>
		</model>
