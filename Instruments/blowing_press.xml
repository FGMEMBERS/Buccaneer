<?xml version="1.0"?>

<!-- air speed indicator -->

<PropertyList>

	<path>blowing_press.ac</path>

	<animation>
		<type>rotate</type>
		<object-name>needle1</object-name>
		<property>engines/engine[0]/bleed-air-pressure-psi</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-120</dep>
			</entry>
			<!--<entry>
				<ind>83</ind>
				<dep>0</dep>
			</entry>-->
			<entry>
				<ind>120</ind>
				<dep>120</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0.0394015</x-m>
			<y-m>-0.0401415</y-m>
			<z-m>0.000231282</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>needle2</object-name>
		<property>engines/engine[1]/bleed-air-pressure-psi</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-120</dep>
			</entry>
			<!--<entry>
				<ind>83</ind>
				<dep>0</dep>
			</entry>-->
			<entry>
				<ind>120</ind>
				<dep>120</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0.0394015</x-m>
			<y-m>0.0401415</y-m>
			<z-m>0.000231282</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- *** LoD *** -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>80</max-m>
	</animation>

</PropertyList>
