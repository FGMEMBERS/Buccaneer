<?xml version="1.0"?>

<!-- Flap and Aileron Droop control -->

<PropertyList>

 <path>flap.ac</path>

 <!-- Panel Lighting -->
 <animation> 
  <type>material</type>
  <object-name>bezelinside</object-name>
  <object-name>face</object-name>
  <emission> 
   <factor-prop>systems/electrical/outputs/instr-lights-port</factor-prop>
   <red>0.03125</red>
   <green>0.0089</green>
   <blue>0.0089</blue>
  </emission>
 </animation>
 
<!-- *** LoD *** -->
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5</max-m>
 </animation> 

</PropertyList>
